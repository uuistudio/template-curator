---
let {icons} = Astro.props
---
{ icons && 
<div class="text-center wow fadeInUp" data-wow-delay=".3s">
  <img
    src= {icons.image}
    alt="image"
    class="
      max-w-[250px]
      w-full
      mx-auto
      opacity-50
      hover:opacity-100
      transition
      duration-300
      ease-in-out
    "
  />
</div>
}