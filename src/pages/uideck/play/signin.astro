---
import Bare from "@template/layouts/Bare.astro";

import Banner from "@template/components/Banner.astro";
import SignIn from "@template/components/SignIn.astro";
import Logo from '@template/components/Logo.astro'

import SiteData from "@src/utils.js"
let site = await SiteData('../data')
let title = "Sign In Page";
let meta = {
  title: title,
};
---

<Bare {meta}>
  <Fragment slot="logo">
    <Logo logo={site.logo} />
  </Fragment>
  <Banner {title} slot="primary" />
  <SignIn slot="content" />
</Bare>
