---
import Base from "../layouts/Base.astro";
import Navbar from "../components/Navbar/index.astro";
import Contact from "../components/Contact/index.astro";
import Footer from "../components/Footer.astro";

let { meta } = Astro.props;
---

<Base {meta}>
  <Navbar>
    <Fragment slot="logo">
      <slot name="logo" />
    </Fragment>
  </Navbar>
  <slot name="primary" />
  <slot name="content" />
  <Contact />
  <Footer />
</Base>
